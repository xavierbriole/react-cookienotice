{
  "name": "react-cookienotice",
  "version": "4.1.0",
  "description": "A lightweight & customizable cookie banner for your React App",
  "author": "xavierbriole",
  "license": "MIT",
  "homepage": "https://react-cookienotice.vercel.app",
  "repository": "xavierbriole/react-cookienotice",
  "keywords": [
    "banner",
    "cookie banner",
    "cookie notice",
    "cookies",
    "gdpr",
    "notice",
    "react",
    "rgpd"
  ],
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "format": "prettier --write .",
    "prepare": "run-s build",
    "test": "run-s test:unit test:format test:lint test:build",
    "test:build": "run-s build",
    "test:format": "prettier --check .",
    "test:lint": "eslint .",
    "test:unit": "cross-env TZ=America/New_York CI=1 react-scripts test --env=jsdom --colors --coverage",
    "test:watch": "react-scripts test --env=jsdom"
  },
  "peerDependencies": {
    "react": "^16.0.0 || ^17.0.0",
    "react-dom": "^16.0.0 || ^17.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.8",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.28.0",
    "microbundle-crl": "^0.13.10",
    "mockdate": "^3.0.5",
    "npm-run-all": "^4.1.5",
    "postcss-normalize": "^10.0.1",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^5.0.0",
    "typescript": "^4.5.4"
  },
  "files": [
    "dist"
  ]
}
