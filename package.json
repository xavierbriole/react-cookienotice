{
  "name": "react-cookienotice",
  "version": "5.0.1",
  "description": "A lightweight & customizable cookie banner for your React App",
  "author": "xavierbriole",
  "license": "MIT",
  "homepage": "https://react-cookienotice.vercel.app",
  "repository": "xavierbriole/react-cookienotice",
  "keywords": [
    "banner",
    "cookie banner",
    "cookie notice",
    "cookies",
    "gdpr",
    "notice",
    "react",
    "rgpd"
  ],
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.tsx",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "format": "prettier --write .",
    "prepare": "run-s build",
    "test": "run-s test:unit test:format test:lint test:build",
    "test:build": "run-s build",
    "test:format": "prettier --check .",
    "test:lint": "eslint .",
    "test:unit": "cross-env TZ=America/New_York CI=1 react-scripts test --env=jsdom --colors --coverage --collectCoverageFrom='[\"src/**/*.{ts,tsx}\",\"!src/**/*.stories.tsx\"]'",
    "test:watch": "cross-env TZ=America/New_York react-scripts test --env=jsdom",
    "start-storybook": "start-storybook -c .storybook -p 9001",
    "build-storybook": "build-storybook -c .storybook -o build/storybook"
  },
  "peerDependencies": {
    "react": "^16.0.0 || ^17.0.0 || ^18.0.0",
    "react-dom": "^16.0.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@storybook/addon-actions": "^6.5.3",
    "@storybook/addon-essentials": "^6.5.3",
    "@storybook/addon-links": "^6.5.3",
    "@storybook/react": "^6.5.3",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.35",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.5",
    "cross-env": "^7.0.3",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.30.0",
    "microbundle-crl": "^0.13.11",
    "mockdate": "^3.0.5",
    "npm-run-all": "^4.1.5",
    "postcss-normalize": "^10.0.1",
    "prettier": "^2.6.2",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-scripts": "^5.0.1",
    "storybook-css-modules-preset": "^1.1.1",
    "storybook-dark-mode": "^1.1.0",
    "typescript": "^4.6.4"
  },
  "files": [
    "dist"
  ],
  "volta": {
    "node": "16.13.2"
  },
  "packageManager": "yarn@3.2.0"
}
