{
  "name": "react-cookienotice",
  "version": "0.0.1",
  "description": "An awesome cookie banner for your website",
  "author": "xavierbriole",
  "license": "MIT",
  "homepage": "https://xavierbriole.github.io/react-cookienotice",
  "repository": "xavierbriole/react-cookienotice",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "flow": "flow check --color=always",
    "prepare": "run-s build",
    "test": "run-s test:unit test:format test:lint test:build",
    "test:build": "run-s build",
    "test:format": "prettier --check \"src/**/*.js\"",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "classnames": "^2.2.6",
    "js-cookie": "^2.2.1",
    "react": "^16.0.0",
    "styled-components": "^4.4.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "classnames": "^2.2.6",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "flow-bin": "^0.126.1",
    "gh-pages": "^2.2.0",
    "js-cookie": "^2.2.1",
    "microbundle-crl": "^0.13.10",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "styled-components": "^4.4.1"
  },
  "files": [
    "dist"
  ]
}
